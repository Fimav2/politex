import csv

with open("students.csv",encoding="utf8") as file:
    reader = csv.reader(file, delimiter=",")
    answer = list(reader)


for i in range(len(answer)):
    if answer[i][1] == "Name":
        answer[i].append("login")
    else:
        sp = answer[i][1].split()
        otv = sp[0] + "_" + sp[1][0]
        answer[i].append(otv)
    print(answer)
